public class row {
    String P1;
    String P2;
    String exp;
    String type;
    String author;
    public row(String P1, String P2, String exp, String type, String author){
     this.P1 =P1;
     this.P2 = P2;
     this.author = author;
     this.exp = exp;
     this.type = type;
    }
    public String getP1(){
    return P1;
    }
    public String getP2(){
    return P2;
    }
    public String getType(){
    return type;
    }
    public String getexp(){
        return exp;
    }
    public String getauthor(){
        return author;
    }
    public String getInfo(){
    return author + "." + exp;
    
    }
    @Override
    public boolean equals(Object p){
    if (p == null){
        return false;
    }
    if(!(p instanceof row)){
        return false;}
    row p2 = (row) p;
    return P1.equals(p2.getP1()) && P2.equals(p2.getP2()) || P1.equals(p2.getP2()) && P2.equals(p2.getP1());
    }
    @Override
    public int hashCode(){
        return P1.hashCode() + P2.hashCode();
    }
    
    
}
